{
    "version": 2,
    "builds": [
        {
            "src": "toys-store/wsgi.py",
            "use": "@devjin8/vercel-poetry@latest",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "toys-store/wsgi.py"
        }
    ]
}